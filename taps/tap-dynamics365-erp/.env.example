###############################################################################
# .env.example - Environment variables for Singer tap-dynamics365-erp
#
# Copy this to .env and fill in your values:
#   cp .env.example .env
#
# These values are used by docker-compose.yml and schedule.sh.
# Sensitive credentials should go in config/config.json, NOT here.
###############################################################################

# --- Scheduling ---
# How often to run syncs when using the "scheduled" service (in seconds)
SYNC_INTERVAL_SECONDS=3600

# --- Target selection for scheduled runs ---
# Which Singer target to pipe into (must be installed in the image)
SYNC_TARGET=target-csv
SYNC_TARGET_CONFIG=target_csv_config.json

# --- Logging ---
# Directory for sync log files inside the container
SINGER_LOG_DIR=/app/logs
